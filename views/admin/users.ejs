<%- include('../partials/header'); -%>
<h2>Админ панель - пользователи</h2>
<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Имя</th>
      <th scope="col">Почта</th>
      <th scope="col">Роль</th>
    </tr>
  </thead>
  <tbody>
    <% for(const user of users) {%>
      <tr>
        <th scope="row"><%=user.id %></th>
        <td><%=user.name %></td>
        <td><%=user.email %></td>
        <td><%=user.role %></td>
      </tr>
    <%} %>
  </tbody>
</table>
<%- include('../partials/footer'); -%>
